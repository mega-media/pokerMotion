{
  "name": "PockerMotion",
  "version": "1.0.0",
  "description": "Pocker Motion Example",
  "main": "index.js",
  "scripts": {
    "flow-install": "flow-typed install --overwrite",
    "flow-test": "flow || notify -t 'Flow Type' -m 'something mistake'",
    "flow-test:watch": "npm run flow-test; fswatch -e ./**/* -o 1 | xargs -n1 -I{} sh -c 'tput setaf 6; date \"+\n[%m/%d %H:%M:%S]\";tput sgr0; npm run flow-test'",
    "mocha-test": "cross-env NODE_PATH=./app/dist NODE_ENV=test mocha --opts ./.mochaopts \"__test__/**/*.test.*\" || exit 0",
    "mocha-test:watch": "cross-env NODE_PATH=./app/dist NODE_ENV=test mocha --watch --opts ./.mochaopts \"__test__/**/*.test.*\"",
    "start": "cross-env NODE_ENV=development npm run build && webpack-dev-server --devtool eval --progress --colors --hot --content-base build",
    "copy": "cp app/index.html build/index.html && cp -a app/assets/ build/assets",
    "build": "webpack && npm run copy"
  },
  "author": "shown",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.9.1",
    "babel-loader": "^6.2.4",
    "babel-plugin-syntax-flow": "^6.18.0",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-3": "^6.22.0",
    "babel-runtime": "^6.23.0",
    "chai": "^3.5.0",
    "expose-loader": "^0.7.3",
    "flow-bin": "^0.41.0",
    "flow-typed": "^2.0.0",
    "growl": "^1.9.2",
    "mocha": "^3.2.0",
    "node-notifier": "^5.0.2",
    "node-notifier-cli": "^1.0.1",
    "script-loader": "^0.7.0",
    "sinon": "^1.17.7",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1",
    "webpack-notifier": "^1.5.0"
  },
  "dependencies": {
    "babel-polyfill": "^6.23.0",
    "cross-env": "^3.2.3",
    "es6-promise": "^4.1.0",
    "keymirror": "^0.1.1",
    "path": "^0.12.7",
    "phaser": "2.6.2"
  }
}
